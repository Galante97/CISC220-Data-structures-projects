#include "NodeTB.hpp"

#include <iostream>
#include <stdlib.h>
using namespace std;


NodeTB::LLNode(char x) {
	data = x;
	count = NULL;
	code = NULL;

	next = NULL;
	prev = NULL;

	left = NULL;
	right = NULL;


} //constructor

LLNode::~LLNode() {
	if (next != NULL) {
		cout << "delete" << endl;

	}
}

